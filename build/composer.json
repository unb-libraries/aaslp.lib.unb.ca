{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Drupal 8 component of aaslp.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library",
                "harvesthq/chosen"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "installer-types": [
            "library"
        ],
        "patches": {}
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "name": "unb-libraries/aaslp.lib.unb.ca",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "package": {
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/simeydotme/jQuery-ui-Slider-Pips/archive/v1.11.4.zip"
                },
                "name": "simeydotme/jquery-ui-slider-pips",
                "require": {
                    "composer/installers": "^1.2.0"
                },
                "type": "drupal-library",
                "version": "1.11.4"
            },
            "type": "package"
          }
    ],
    "require": {
        "drupal/admin_toolbar": "3.0.0",
        "drupal/anchor_link": "2.5",
        "drupal/bootstrap4": "2.1.6",
        "drupal/chosen": "3.0.1",
        "drupal/core": "8.9.13",
        "drupal/entity_reference_revisions": "1.9",
        "drupal/facets": "1.7",
        "drupal/google_analytics": "2.5",
        "drupal/health_check": "1.3",
        "drupal/inline_entity_form": "1.0-rc9",
        "drupal/metatag": "1.15",
        "drupal/node_edit_protection": "1.0",
        "drupal/pathauto": "1.8",
        "drupal/redis": "1.5",
        "drupal/samlauth": "3.0-rc2",
        "drupal/search_api": "1.19",
        "drupal/search_api_solr": "4.1.11",
        "drupal/simple_sitemap": "3.8",
        "drupal/token": "1.9",
        "drupal/twig_tweak": "2.9",
        "oomphinc/composer-installers-extender": "1.1.2",
        "unb-libraries/drupal-builder": "^8.0.0",
        "unb-libraries/saml_features": "dev-8.x-1.x"
    },
    "require-dev": {
        "unb-libraries/drupal-development-libraries": "^8.0.0"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
