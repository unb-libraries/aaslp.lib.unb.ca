{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Drupal 8 component of aaslp.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/ckeditor/plugins/{$name}": [
                "vendor:ckeditor-plugin"
            ],
            "libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {
            "drupal/samlauth": {
                "Ignore role restrictions": "/build/patches/samlauth_rolematch.patch"
            },
            "drupal/facets": {
                "Fix range_slider jquery_ui D9": "/build/patches/facets_slider_d9.patch",
                "Fix Views AJAX Facets D9": "https://www.drupal.org/files/issues/2018-09-25/ajax_facet_block_views_context-2986981-13.patch"
            }
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "name": "unb-libraries/aaslp.lib.unb.ca",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.7",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "furf/jquery-ui-touch-punch",
                "version": "dev-master",
                "type": "drupal-library",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/furf/jquery-ui-touch-punch/archive/refs/heads/master.zip"
                }
            }
        },
        {
            "package": {
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/simeydotme/jQuery-ui-Slider-Pips/archive/v1.11.4.zip"
                },
                "name": "simeydotme/jquery-ui-slider-pips",
                "require": {
                    "composer/installers": "^1.2.0"
                },
                "type": "drupal-library",
                "version": "1.11.4"
            },
            "type": "package"
        },
        {
            "package": {
                "dist": {
                    "type": "zip",
                    "url": "https://download.ckeditor.com/widget/releases/widget_4.16.1.zip"
                },
                "name": "ckeditor-plugin/widget",
                "require": {
                    "composer/installers": "^1.2.0"
                },
                "type": "drupal-library",
                "version": "4.16.1"
            },
            "type": "package"
        }
    ],
    "require": {
        "ckeditor-plugin/widget": "4.16.1",
        "drupal/admin_toolbar": "3.1.0",
        "drupal/address": "1.10",
        "drupal/bootstrap4": "2.1.14",
        "drupal/chosen": "3.0.1",
        "drupal/core": "9.3.4",
        "drupal/easy_breadcrumb": "2.0.2",
        "drupal/entity_reference_revisions": "1.9",
        "drupal/facets": "2.0.1",
        "drupal/fontawesome": "2.19",
        "drupal/geocoder": "3.25",
        "drupal/geocoder_ajax_prepopulate": "2.0",
        "drupal/geofield": "1.36",
        "drupal/geofield_map": "2.71",
        "drupal/google_analytics": "4.0",
        "drupal/health_check": "1.3",
        "drupal/inline_entity_form": "1.0-rc9",
        "drupal/jquery_ui": "1.4",
        "drupal/jquery_ui_slider": "1.1",
        "drupal/jquery_ui_touch_punch": "1.0.0",
        "drupal/leaflet": "2.1.20",
        "drupal/leaflet_more_maps": "2.1.2",
        "drupal/node_edit_protection": "1.0",
        "drupal/pathauto": "1.8",
        "drupal/redis": "1.5",
        "drupal/samlauth": "3.4",
        "drupal/search_api": "1.23",
        "drupal/search_api_solr": "4.2.6",
        "drupal/token": "1.10",
        "drupal/twig_tweak": "2.9",
        "drupal/views_timelinejs": "3.2",
        "furf/jquery-ui-touch-punch": "dev-master",
        "geocoder-php/nominatim-provider": "5.5.0",
        "harvesthq/chosen": "^1.8",
        "oomphinc/composer-installers-extender": "^2",
        "simeydotme/jquery-ui-slider-pips": "~1.11",
        "unb-libraries/drupal-builder": "^9",
        "unb-libraries/saml_features": "dev-9.x-1.x"
    },
    "require-dev": {
        "unb-libraries/drupal-development-libraries": "^9"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
